{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "User",
  "description": "The user JSON document.",
  "required": ["name", "password"],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The id."
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The name of the user.",
      "examples": ["Jon Snow"]
    },
    "password": {
      "$id": "#/properties/password",
      "type": "string",
      "title": "The password of the user."
    },

    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the user."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date user last updated."
    }
  },
  "additionalProperties": false
}
